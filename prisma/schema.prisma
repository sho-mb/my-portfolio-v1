generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Portfolio {
  id      Int    @id @default(autoincrement())
  title   String
  content String
  url     String
  alt     String
  path    String
  height  Int
  width   Int
}

model user {
  id       Int    @id @default(autoincrement())
  identify String
  password String
}
